<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
    @Html.DevExpress().GetStyleSheets(new StyleSheet { ExtensionSuite = ExtensionSuite.Chart })
    @Html.DevExpress().GetScripts(new Script { ExtensionSuite = ExtensionSuite.Chart })
</head>
<body>
    @RenderBody()
</body>
</html>
